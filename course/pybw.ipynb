   "source": [
    "# bw 7. range보다는 enumerate 사용\n",
    "from random import randint\n",
    "\n",
    "random_bits = 0\n",
    "for i in range(32):\n",
    "    if randint(0, 1):\n",
    "        random_bits |= 1 << i\n",
    "\n",
    "print(random_bits)"
   ]
